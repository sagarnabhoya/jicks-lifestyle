<!doctype html>
<html class="no-js" lang="{{ request.locale.iso_code }}"> 
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <meta name="theme-color" content="">
    <link rel="canonical" href="{{ canonical_url }}">
    <link rel="preconnect" href="https://cdn.shopify.com" crossorigin>
    {%- render 'seo-title' -%}
    {% render 'meta-tags' %}
    {{ content_for_header }}
    {%- unless settings.use_body_google_font == true or settings.alt_google_fonts == true -%}
      <link rel="preconnect" href="https://fonts.shopifycdn.com" crossorigin>
    {%- endunless -%}
    {%- if settings.use_body_google_font == true or settings.alt_google_fonts == true -%}
      <link rel="preconnect" href="https://fonts.google.com/" rel="stylesheet">
    {%- endif -%}
    {{ 'font-icons.min.css' | asset_url | stylesheet_tag }}
    {{ 'theme-vendor.min.css' | asset_url | stylesheet_tag }}
    {{ 'base.min.css' | asset_url | stylesheet_tag }}   
    {% render 'css-variables' %}
    {%- if settings.use_body_google_font == true -%}
      <link href="https://fonts.googleapis.com/css?family={{ settings.body_google_fonts }}:100,200,300,400,500,600,700,800,900&display=swap" rel="stylesheet" defer>
    {%- endif -%}
    {%- if settings.alt_google_fonts == true -%}
      <link href="https://fonts.googleapis.com/css?family={{ settings.alt_google_fonts }}:100,200,300,400,500,600,700,800,900&display=swap" rel="stylesheet" defer>
    {%- endif -%}
    <script>document.documentElement.className = document.documentElement.className.replace('no-js', 'js');
    if (Shopify.designMode) {
      document.documentElement.classList.add('shopify-design-mode');
    }
    </script>   
  </head>
  <body class="password">
    <main id="MainContent" class="password-main">
      {{ content_for_layout }}
    </main>
    {%- render 'internal-style' -%} 
  </body>
</html>
