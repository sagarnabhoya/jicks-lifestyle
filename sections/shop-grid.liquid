<div
  id="{{ section.settings.section_id }}"
  class="section-bg overflow-hidden section-{{ section.id }} {% if section.settings.no_padding %} px-0{% endif %}{% if section.settings.section_show == false %} d-none{% endif %} d-md-block {{ section.settings.section_class_name }} {{ section.settings.padding_top }} {{ section.settings.padding_bottom }}"
  style="--section_bg: {% if section.settings.section_gradient != blank %}{{ section.settings.section_gradient }}{% else %}{{ section.settings.section_bg_color }}{% endif %}"
>
  <div class="{{ section.settings.container_type }} {% if section.settings.gutter_size == 'gutter-none' %} p-0{% endif %}">
    {%- if section.settings.heading != blank
      or section.settings.sub_heading != blank
      or section.settings.description != blank
    -%}
      <div class="row heading-wrapper title-{{ section.settings.title_style }} align-items-{% if section.settings.description != blank %}start{% else %}center{% endif %} text-center text-lg-start">
        {%- render 'section-title-style', heading_alignment: section.settings.heading_alignment -%}
      </div>
    {%- endif -%}
    {%- for block in section.blocks -%}
      {%- assign block_name = block.type -%}
    {%- endfor -%}
    <div class="row {% if section.settings.gutter_size == 'gutter-none' %}g-0{% endif %}">
      <div class="col-12">
        <ul class="{% unless block_name == 'shop-grid-style-3' or block_name == 'shop-grid-style-4' or block_name == 'shop-grid-style-5' %}grid-isotop {{ section.settings.gutter_size }}{% endunless %} shop-grid grid  {{ block_name }}">
          {%- unless block_name == 'shop-grid-style-3' or block_name == 'shop-grid-style-4' or block_name == 'shop-grid-style-5'-%}
            <li class="grid-sizer"></li>
          {%- endunless -%}
          {%- liquid
            for block in section.blocks
              case block.type
                when 'shop-grid-style-1'
                  render 'shop-grid-style-1', block_settings: block.settings, section: section.settings, grid_style: block.type
                when 'shop-grid-style-2'
                  render 'shop-grid-style-2', block_settings: block.settings, section: section.settings
                when 'shop-grid-style-3'
                  render 'shop-grid-style-1', block_name: block_name, block_settings: block.settings, section: section.settings, grid_style: block.type
                when 'shop-grid-style-4'
                  render 'shop-grid-style-4', block_name: block_name, block_settings: block.settings, section: section.settings, grid_style: block.type
                when 'shop-grid-style-5'
                  render 'shop-grid-style-5', block_name: block_name, block_settings: block.settings, section: section.settings, grid_style: block.type  
              endcase
            endfor
          -%}
        </ul>
      </div>
    </div>
  </div>
</div>
